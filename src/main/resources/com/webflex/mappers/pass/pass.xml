<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Pass">
	<select id="pass_name" resultType="p">
		select * from wf_pass where pass_no = #{pass_no}
	</select>
	
	<select id="pass_info" resultType="p">
		select * from wf_pass where pass_no = #{pass_no}
	</select>
	
	<select id="pass_list" resultType="p">
		select * from wf_pass order by pass_no desc
	</select>
</mapper>